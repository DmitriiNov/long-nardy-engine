{
  "name": "long-nardy-engine",
  "version": "0.3.0",
  "description": "Long nardy game engine",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "test": "jest",
    "coverage": "jest --coverage",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "version": "npm run format && git add -A src",
    "preversion": "npm run lint",
    "postversion": "git push && git push --tags"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DmitriiNov/long-nardy-engine.git"
  },
  "keywords": [
    "Backgammon",
    "Nardy",
    "Long nardy"
  ],
  "author": "DmitriiNov",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DmitriiNov/long-nardy-engine/issues"
  },
  "homepage": "https://github.com/DmitriiNov/long-nardy-engine#readme",
  "devDependencies": {
    "@types/node": "^18.11.18",
    "jest": "^29.5.0",
    "nyc": "^15.1.0",
    "prettier": "^2.8.8",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^5.0.4"
  }
}
